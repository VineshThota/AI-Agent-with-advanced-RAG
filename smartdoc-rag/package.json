    "openai": "^4.20.1",
    "@pinecone-database/pinecone": "^1.1.2",
    "sentence-transformers": "^1.0.0",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "joi": "^17.11.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
